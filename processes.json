{
  "apps": [
    {
      "name": "wpzf",
      "cwd": "./logs",
      "script": "bin/www",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "error_file": "log/wpzf.stderr.log",
      "out_file": "log/wpzf.stdout.log",
      "pid_file": "pids/wpzf-geo-api.pid",
      "instances": 6,
      "min_uptime": "200s",
      "max_restarts": 10,
      "max_memory_restart": "1M",
      "cron_restart": "1 0 * * *",
      "watch": false,
      "merge_logs": true,
      "exec_interpreter": "node",
      "exec_mode": "cluster",
      "autorestart": true,
      "vizion": false
    }
  ]
}
